{
    "name": "rtckit/eqivo",
    "description": "Telephony API Platform",
    "version": "0.6.2",
    "keywords": [
        "telecommunications",
        "voip",
        "telephony",
        "telco",
        "voice",
        "freeswitch",
        "api"
    ],
    "homepage": "https://github.com/rtckit/eqivo",
    "license": "MIT",
    "authors": [
        {
            "name": "Ciprian Dosoftei"
        }
    ],
    "support": {
        "email": "hello@rtckit.io",
        "issues": "https://github.com/rtckit/eqivo/issues"
    },
    "require": {
        "php": ">=8.1.0",
        "ext-ctype": "*",
        "ext-date": "*",
        "ext-filter": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-pcre": "*",
        "ext-simplexml": "*",
        "monolog/monolog": "^3.5",
        "nikic/fast-route": "^1.3",
        "ramsey/uuid": "^4.7",
        "react/event-loop": "^1.5",
        "react/http": "^1.9",
        "react/promise": "^3.1",
        "rtckit/esl": "^0.8",
        "rtckit/ficore": "v0.0.3",
        "rtckit/sip": "^0.7",
        "symfony/yaml": "^6.3",
        "wikimedia/ip-set": "^4.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "vimeo/psalm": "^5.16"
    },
    "autoload": {
        "psr-4": {
            "RTCKit\\Eqivo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RTCKit\\Eqivo\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/eqivo"
    ],
    "config": {
        "allow-plugins": false,
        "platform": {
            "php": "8.1"
        }
    },
    "scripts": {
        "phpstan": "php -d memory_limit=-1 ./vendor/bin/phpstan analyse -c ./etc/phpstan.neon -n -vvv --ansi --level=max src",
        "psalm": "php -d memory_limit=-1 ./vendor/bin/psalm --config=./etc/psalm.xml --show-info=true",
        "dev-docker": "docker build -f ./etc/Dockerfile.dev -t rtckit/eqivo ."
    }
}
